{
  "name": "@easypost/api",
  "description": "EasyPost Node Client Library",
  "version": "5.1.0",
  "author": "Easypost Engineering <oss@easypost.com>",
  "homepage": "https://easypost.com",
  "bin": {
    "easypost": "./repl.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/easypost/easypost-node.git"
  },
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">= 10.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.babel.js",
    "check": "prettier --check .",
    "clean": "rm -rf ./dist ./nyc_output ./node_modules/.cache ./coverage",
    "coverage": "cross-env NODE_ENV=test nyc npm run test",
    "fix": "eslint src test --fix",
    "format": "prettier --write .",
    "lint": "eslint src test",
    "prepublishOnly": "npm run clean && npm run build && npm run test && npm run lint && npm run check",
    "repl": "./repl.js --local easypost.js",
    "test": "cross-env NODE_ENV=test mocha --timeout 10000 --require @babel/register --require ./test/common.js --recursive ./test",
    "watch": "webpack --config webpack.config.babel.js --watch"
  },
  "dependencies": {
    "core-js": "3.19.3",
    "nodent-runtime": "3.2.1",
    "proptypes": "1.1.0",
    "regenerator-runtime": "0.13.9",
    "source-map-support": "0.5.21",
    "superagent": "7.1.1",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-syntax-export-extensions": "^7.0.0-beta.32",
    "@babel/plugin-transform-react-constant-elements": "^7.16.7",
    "@babel/plugin-transform-react-inline-elements": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@easypost/eslint-config-easypost-base": "~2.2.3",
    "@pollyjs/adapter-node-http": "^6.0.4",
    "@pollyjs/core": "^6.0.4",
    "@pollyjs/persister-fs": "^6.0.4",
    "babel-loader": "~8.2.3",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "~0.12.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "~2.25.2",
    "eslint-plugin-jest": "~22.21.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.3",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "vows": "^0.8.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  }
}
